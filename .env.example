# REQUIRED: SmartProxy credentials
SMARTPROXY_USERNAME="your_smartproxy_username"
SMARTPROXY_PASSWORD="your_smartproxy_password"

# API Configuration
API_HOST="0.0.0.0"                          
API_PORT=8000
DEBUG=true

# Rate Limiting
