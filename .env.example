MAX_CONCURRENT_REQUESTS=5

# Instagram settings
INSTAGRAM_POST_ANALYSIS_LIMIT=50
INSTAGRAM_FOLLOWER_SAMPLE_SIZE=1000

# Redis (for caching) - optional
REDIS_URL="redis://localhost:6379/0"